#!/usr/bin/env python3

import logging
import os
from   pprint import pprint

import apidoc.inspector

if __name__ == "__main__":
   logging.getLogger().setLevel(logging.DEBUG)

   path = os.path.dirname(apidoc.__file__)
   inspector = apidoc.inspector.Inspector(path)
   pprint(inspector.inspect(apidoc.__name__, apidoc))



